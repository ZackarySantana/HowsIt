---
export interface Props {
    type: string;
    title?: string;
    typeInfo?: string[];
    exampleInfo?: string[];
}
import Card from "./card.astro";
const { type, title, typeInfo, exampleInfo } = Astro.props;
---

<Card
    metaTitle="(SRR Only)"
    hideClientHydrated={true}
    hideClientRendered={true}
    type={type}
    title={title}
    typeInfo={typeInfo}
    exampleInfo={exampleInfo}
>
    <slot slot="ssr" />
</Card>
