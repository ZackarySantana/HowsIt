---
import Card from "./card.astro";
const { example, type, title, typeInfo, exampleInfo } = Astro.props;
---

<Card
    metaTitle="(SRR Only)"
    hideClientHydrated={true}
    hideClientRendered={true}
    example={example}
    type={type}
    title={title}
    typeInfo={typeInfo}
    exampleInfo={exampleInfo}
>
    <slot slot="ssr" />
</Card>
